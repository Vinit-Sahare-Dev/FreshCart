[32m+[m[32mimport { useState, useEffect } from 'react';[m
[32m+[m[32mimport './AdminHome.css';[m
[32m+[m
[32m+[m[32mfunction AdminHome() {[m
[32m+[m[32m  const [user, setUser] = useState(null);[m
[32m+[m[32m  const [stats, setStats] = useState({[m
[32m+[m[32m    totalDishes: 0,[m
[32m+[m[32m    totalOrders: 0,[m
[32m+[m[32m    totalRevenue: 0,[m
[32m+[m[32m    totalUsers: 0,[m
[32m+[m[32m    pendingOrders: 0,[m
[32m+[m[32m    todayOrders: 0[m
[32m+[m[32m  });[m
[32m+[m[32m  const [recentOrders, setRecentOrders] = useState([]);[m
[32m+[m[32m  const [recentActivity, setRecentActivity] = useState([]);[m
[32m+[m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    const token = localStorage.getItem('hotel_jwt');[m
[32m+[m[32m    if (token) {[m
[32m+[m[32m      try {[m
[32m+[m[32m        const base64Url = token.split('.')[1];[m
[32m+[m[32m        const base64 = base64Url.replace(/-/g, '+').replace(/_/g, '/');[m
[32m+[m[32m        const jsonPayload = decodeURIComponent([m
[32m+[m[32m          atob(base64).split('').map(c => '%' + ('00' + c.charCodeAt(0).toString(16)).slice(-2)).join('')[m
[32m+[m[32m        );[m
[32m+[m[32m        const userData = JSON.parse(jsonPayload);[m
[32m+[m[32m        setUser(userData);[m
[32m+[m[41m        [m
[32m+[m[32m        if (userData.role !== 'ADMIN') {[m
[32m+[m[32m          window.location.href = '/';[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m[32m        fetchDashboardData();[m
[32m+[m[32m      } catch (err) {[m
[32m+[m[32m        console.error('Error parsing token:', err);[m
[32m+[m[32m        window.location.href = '/';[m
[32m+[m[32m      }[m
[32m+[m[32m    } else {[m
[32m+[m[32m      window.location.href = '/';[m
[32m+[m[32m    }[m
[32m+[m[32m  }, []);[m
[32m+[m
[32m+[m[32m  const fetchDashboardData = async () => {[m
[32m+[m[32m    try {[m
[32m+[m[32m      const token = localStorage.getItem('hotel_jwt');[m
[32m+[m[41m      [m
[32m+[m[32m      // Fetch dishes[m
[32m+[m[32m      const dishRes = await fetch('http://localhost:8080/api/dishes', {[m
[32m+[m[32m        headers: { 'Authorization': `Bearer ${token}` }[m
[32m+[m[32m      });[m
[32m+[m[32m      const dishData = await dishRes.json();[m
[32m+[m
[32m+[m[32m      // Fetch orders[m
[32m+[m[32m      const orderRes = await fetch('http://localhost:8080/api/orders', {[m
[32m+[m[32m        headers: { 'Authorization': `Bearer ${token}` }[m
[32m+[m[32m      });[m
[32m+[m[32m      const orderData = await orderRes.json();[m
[32m+[m
[32m+[m[32m      // Calculate stats[m
[32m+[m[32m      const totalRevenue = orderData.reduce((sum, order) => sum + (order.totalAmount || 0), 0);[m
[32m+[m[32m      const pendingOrders = orderData.filter(o => o.status === 'PENDING').length;[m
[32m+[m[32m      const today = new Date().toDateString();[m
[32m+[m[32m      const todayOrders = orderData.filter(o =>[m[41m [m
[32m+[m[32m        new Date(o.orderDate || Date.now()).toDateString() === today[m
[32m+[m[32m      ).length;[m
[32m+[m
[32m+[m[32m      setStats({[m
[32m+[m[32m        totalDishes: dishData.length,[m
[32m+[m[32m        totalOrders: orderData.length,[m
[32m+[m[32m        totalRevenue: totalRevenue,[m
[32m+[m[32m        totalUsers: 150,[m
[32m+[m[32m        pendingOrders: pendingOrders,[m
[32m+[m[32m        todayOrders: todayOrders[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      // Set recent orders (last 5)[m
[32m+[m[32m      setRecentOrders(orderData.slice(0, 5));[m
[32m+[m
[32m+[m[32m      // Mock recent activity[m
[32m+[m[32m      setRecentActivity([[m
[32m+[m[32m        { id: 1, action: 'New order placed', user: 'John Doe', time: '2 min ago', icon: 'üì¶', color: '#10b981' },[m
[32m+[m[32m        { id: 2, action: 'Dish updated', dish: 'Butter Chicken', time: '15 min ago', icon: '‚úèÔ∏è', color: '#f59e0b' },[m
[32m+[m[32m        { id: 3, action: 'Order delivered', user: 'Jane Smith', time: '1 hour ago', icon: '‚úì', color: '#059669' },[m
[32m+[m[32m        { id: 4, action: 'New user registered', user: 'Mike Wilson', time: '2 hours ago', icon: 'üë§', color: '#3b82f6' },[m
[32m+[m[32m        { id: 5, action: 'Payment received', amount: '‚Çπ850', time: '3 hours ago', icon: 'üí∞', color: '#8b5cf6' }[m
[32m+[m[32m      ]);[m
[32m+[m[32m    } catch (error) {[m
[32m+[m[32m      console.error('Error fetching data:', error);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const handleLogout = () => {[m
[32m+[m[32m    localStorage.removeItem('hotel_jwt');[m
[32m+[m[32m    window.location.href = '/';[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const navigateToDashboard = () => {[m
[32m+[m[32m    window.location.href = '/admin/dashboard';[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  if (!user) {[m
[32m+[m[32m    return ([m
[32m+[m[32m      <div style={{ display: 'flex', alignItems: 'center', justifyContent: 'center', minHeight: '100vh' }}>[m
[32m+[m[32m        <div style={{ textAlign: 'center', padding: '2rem' }}>[m
[32m+[m[32m          <div style={{ fontSize: '3rem', marginBottom: '1rem' }}>‚è≥</div>[m
