[32m+[m[32m          gradient: 'linear-gradient(135deg, #f59e0b, #d97706)',[m
[32m+[m[32m          tag: 'ü•õ Dairy & Dessert',[m
[32m+[m[32m          bgColor: '#fef7ed',[m
[32m+[m[32m          borderColor: '#fed7aa'[m
[32m+[m[32m        };[m
